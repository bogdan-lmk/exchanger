.license
  z-index: 0
.heading-license
  background: url('/static/img/general/license-img.png') no-repeat center center
  width: 100%
  height: 149px
  display: flex;
  align-items: center;
  justify-content: center

ol {
  counter-reset: section;
  list-style-type: none;
  padding: 0
}

li::before {
  counter-increment: section;
  content: counters(section,".") " ";
  font-weight: 600
}

.content-subtitle
  font-weight: 600
  text-transform: uppercase;

.content-text-wrap
  margin-top: 20px

.content-list
  margin-bottom: 35px
